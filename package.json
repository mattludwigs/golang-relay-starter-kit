    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-relay-plugin": "0.2.3",
    "babel-relay-plugin-loader": "^0.7.1",
    "classnames": "^2.1.3",
    "express": "^4.13.1",
    "react": "^0.14.x",
    "react-dom": "^0.14.x",
    "react-relay": "^0.3.1",
    "webpack": "^1.10.5",
    "webpack-dev-server": "^1.10.1"
  }
